<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<StackPane fx:id="rootPane" prefHeight="454.0" prefWidth="1060.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.dillon.fx.view.system.role.AddUserView">
   <children>
      <HBox fx:id="contentPane" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="7.0" HBox.hgrow="ALWAYS">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="5.0">
                     <children>
                        <TextField fx:id="userSearchField" promptText="请输入用户名称" />
                        <TextField fx:id="phoneField" promptText="请输入手机号" />
                        <Button fx:id="searchBut" mnemonicParsing="false" text="查询">
                           <graphic>
                              <FontIcon iconLiteral="fth-search" />
                           </graphic></Button>
                        <Button fx:id="resetBut" mnemonicParsing="false" text="重置">
                           <graphic>
                              <FontIcon iconLiteral="fth-refresh-cw" />
                           </graphic></Button>
                     </children>
                  </HBox>
                  <TableView fx:id="tableView" editable="true" VBox.vgrow="ALWAYS">
                    <columns>
                      <TableColumn fx:id="selCol" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" sortable="false">
                           <graphic>
                              <CheckBox fx:id="selAllCheckBox" mnemonicParsing="false" />
                           </graphic>
                        </TableColumn>
                      <TableColumn fx:id="idCol" prefWidth="141.0" text="用户编号" />
                        <TableColumn fx:id="userNameCol" prefWidth="75.0" text="用户名称" />
                        <TableColumn fx:id="nickNameCol" prefWidth="75.0" text="用户昵称" />
                        <TableColumn fx:id="deptCol" prefWidth="75.0" text="部门" />
                        <TableColumn fx:id="phonenumberCol" prefWidth="75.0" text="手机号码" />
                        <TableColumn fx:id="statusCol" prefWidth="75.0" text="状态" />
                        <TableColumn fx:id="createTimeCol" minWidth="170.0" prefWidth="355.0" text="创建时间" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <HBox alignment="CENTER_RIGHT" maxHeight="30.0" minHeight="30.0" prefHeight="30.0" prefWidth="823.0" spacing="7.0">
                     <children>
                        <Label text="共100条" />
                        <ComboBox prefWidth="150.0">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="10条/页" />
                                    <String fx:value="20条/页" />
                                    <String fx:value="30条/页" />
                                    <String fx:value="50条/页" />
                                </FXCollections>
                            </items>
                        </ComboBox>
                        <Pagination fx:id="pagination" maxHeight="50.0" minHeight="50.0" style="-fx-page-information-visible: false;" />
                        <Label text="前往" />
                        <TextField prefWidth="60.0" />
                        <Label text="页" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <StackPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </StackPane.margin>
      </HBox>
   </children>
</StackPane>
